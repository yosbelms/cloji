{
  "name": "cloji",
  "version": "0.0.3",
  "description": "A small Lisp",
  "keywords": [
    "clojure",
    "lisp",
    "language",
    "evaluate"
  ],
  "repository": "https://github.com/yosbelms/cloji.git",
  "main": "src/index.js",
  "type": "commonjs",
  "bin": {
    "cloji": "./bin/cli.js"
  },
  "scripts": {
    "test": "jasmine",
    "build": "tsc --sourceMap -d",
    "watch": "tsc --sourceMap --watch",
    "clean": "find src -type f \\( -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.d.ts\" -o -name \"*.d.ts.map\" \\) | grep -v \"./node_modules\" | xargs rm",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "perf": "ts-node spec/perf.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.1.3",
    "@types/jasmine": "^4.3.4",
    "@types/node": "^20.3.1",
    "jasmine": "^5.0.2",
    "ts-node": "^10.9.1"
  }
}